{
  "id": "lyynix-more-journal-enrichers",
  "title": "Lyynix: More Journal Enrichers",
  "description": "Beautify your journals, by adding interactive enrichers and get easy access to your prepared stuff!",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "Lyynix",
      "discord": "lyynix"
    }
  ],
  "media": [
    {
      "type": "setup",
      "url": "modules/lyynix-more-journal-enrichers/assets/thumbs/setup.webp",
      "thumbnail": "modules/lyynix-more-journal-enrichers/assets/thumbs/setup.webp"
    }
  ],
  "packs": [
    {
      "name": "lmje-introduction",
      "label": "LMJE Introduction",
      "path": "./packs/journals",
      "type": "JournalEntry"
    }
  ],
  "packFolders": [
    {
      "name": "Lyynix: Journals",
      "sorting": "m",
      "packs": ["lmje-introduction"]
    }
  ],
  "esmodules": ["./scripts/init.js"],
  "styles": ["./styles/style.css"],
  "languages": [
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "lang/de.json"
    },
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#"
}
